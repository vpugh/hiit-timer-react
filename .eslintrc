{
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "no-console": 0,
    "react/no-render-return-value": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "react/jsx-no-bind": [
      0,
      {}
    ],
    "jsx-a11y/interactive-supports-focus": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}